{
  "$schema": "../../schema/gui.json",
  "title": "领地系统",
  "content": "圈地流程：\n1.新建领地\n2.站在领地某个角落选点 a，在另一个角落选点 b\n3.购买或放弃",
  "buttons": [
    {
      "text": "新建领地",
      "action": "/land new"
    },
    {
      "text": "选点a",
      "action": "/land set a"
    },
    {
      "text": "选点b",
      "action": "/land set b"
    },
    {
      "text": "购买",
      "action": "/land buy"
    },
    {
      "text": "放弃",
      "action": "/land giveup"
    },
    {
      "text": "管理",
      "action": "/land gui"
    },
    {
      "text": "OP 管理",
      "action": "/land mgr",
      "onlyOp": true
    },
    {
      "text": "返回",
      "action": "`index"
    }
  ]
}
